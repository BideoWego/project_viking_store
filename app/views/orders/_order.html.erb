<%= render :partial => 'shared/table', :locals => {:table => {
  :headers => ['Key', 'Value'],
  :rows => order.attributes.keys.map do |a|
    if a == 'user_id'
      value = order.user ? link_to('user', user_path(order.user)) : 'No user'
    else
      value = order.attributes[a]
    end
    [a, value]
  end
}} %>
